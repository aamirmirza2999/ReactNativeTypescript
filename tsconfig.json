{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    },
    "types": ["react", "react-native"]
  },
  "include": ["src/**/*", ".storybook/**/*"],
  "exclude": ["node_modules", "babel.config.js"]
}
